{
  "name": "marionette-browserify-todomvc",
  "version": "0.1.0",
  "description": "A TodoMVC sampler application using CoffeeScript, Backbone.Marionette and Browserify",
  "repository": {
    "type": "git",
    "url": "git://github.com/goblindegook/marionette-browserify-todomvc.git"
  },
  "browser": {
    "underscore": "lodash/dist/lodash.js",
    "backbone.marionette.dust": "./src/scripts/shim/backbone.marionette.dust.js"
  },
  "browserify": {
    "transform": [
      "browserify-data",
      "browserify-dustjs",
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "aliasify": {
    "aliases": {
      "underscore": "lodash"
    }
  },
  "browserify-shim": {},
  "devDependencies": {
    "aliasify": "^1.5.1",
    "browser-sync": "~1.3.6",
    "browserify": "~8.0.3",
    "browserify-data": "^0.2.0",
    "browserify-dustjs": "0.0.4",
    "browserify-shim": "~3.8.2",
    "coffee-script": "^1.8.0",
    "coffeeify": "~0.7.0",
    "deamdify": "^0.1.1",
    "del": "^1.1.1",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^0.4.1",
    "gulp-coffeelint": "^0.4.0",
    "gulp-imagemin": "^0.6.2",
    "gulp-jasmine": "^1.0.1",
    "gulp-karma": "0.0.4",
    "gulp-less": "^2.0.1",
    "gulp-notify": "^1.4.2",
    "gulp-plumber": "^0.6.6",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-sass": "^1.1.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "jasmine": "^2.1.1",
    "karma": "^0.12.28",
    "phantomjs": "^1.9.13",
    "pretty-hrtime": "~0.2.1",
    "remapify": "^1.4.4",
    "require-dir": "^0.1.0",
    "sinon": "^1.12.2",
    "uglifyify": "^2.6.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~2.2.1"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone.localstorage": "^1.1.14",
    "backbone.marionette": "^2.3.0",
    "backbone.radio": "^0.8.4",
    "dustjs-helpers": "^1.5.0",
    "dustjs-linkedin": "^2.5.1",
    "jquery": "~2.1.0",
    "lodash": "^2.4.1",
    "todomvc-common": "^0.3.1"
  }
}
